CREATE TABLE Employees (
    ID INT PRIMARY KEY,
    Name VARCHAR(100),
    Department VARCHAR(50),
    Salary DECIMAL(10, 2)
);

INSERT INTO Employees (ID, Name, Department, Salary)
VALUES 
(1, 'Alice', 'HR', 50000),
(2, 'Bob', 'Engineering', 70000),
(3, 'Charlie', 'Marketing', 60000);

SELECT * FROM Employees WHERE Salary > 55000;
